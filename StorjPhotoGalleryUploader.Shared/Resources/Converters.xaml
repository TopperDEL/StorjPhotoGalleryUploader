<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:StorjPhotoGalleryUploader.Converter">

    <SolidColorBrush x:Key="StorjBlue" Color="#286CFF"/>
    <SolidColorBrush x:Key="StorjGray" Color="#A3A3A3"/>

    <converters:TrueIsVisibleConverter x:Key="TrueIsVisibleConverter"/>
    <converters:TrueIsCollapsedConverter x:Key="TrueIsCollapsedConverter"/>
    <converters:DateTimeConverter x:Key="DateTimeConverter"/>
    <converters:TrueIsActiveConverter x:Key="TrueIsActiveConverter" ActiveColor="{StaticResource StorjBlue}" InactiveColor="{StaticResource StorjGray}"/>
</ResourceDictionary>
